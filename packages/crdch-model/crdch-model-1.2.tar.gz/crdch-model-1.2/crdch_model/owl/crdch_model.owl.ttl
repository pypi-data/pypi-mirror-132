            owl:onClass crdch:CodeableConcept ;
            owl:onProperty crdch:method_type ],
        [ a owl:Restriction ;
            owl:maxQualifiedCardinality 1 ;
            owl:onClass crdch:Entity ;
            owl:onProperty crdch:focus ],
        [ a owl:Restriction ;
            owl:maxQualifiedCardinality 1 ;
