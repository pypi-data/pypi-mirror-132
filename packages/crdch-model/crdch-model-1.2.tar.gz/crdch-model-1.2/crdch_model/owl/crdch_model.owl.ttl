crdch:CodeableConcept a owl:Class,
        linkml:ClassDefinition ;
    rdfs:label "CodeableConcept" ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:maxQualifiedCardinality 1 ;
            owl:onClass crdch:CrdchString ;
            owl:onProperty crdch:text ],
        [ a owl:Restriction ;
